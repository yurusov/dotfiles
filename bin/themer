#!/usr/bin/env bash
wal "$@" 
[ $? -eq 0 ]  || exit 1
pywalfox update
pywal-discord
. "/home/litt/.cache/wal/colors.sh"
gsettings set org.freedesktop.ibus.panel xkb-icon-rgba $color1
oomox-cli -o oomox -m all /home/litt/.cache/wal/colors-oomox
oomox-archdroid-icons-cli -o oomox /home/litt/.cache/wal/colors-oomox
pkill dunst
dunst &
spicetify update
wal_steam
